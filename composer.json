{
  "name": "Ffuenf/Ffuenf_Canonical",
  "license": "MIT",
  "description": "This is a extension for Magento Community Edition that adds advanced settings for canonical links.",
  "authors": [
    {
      "name": "Achim Rosenhagen",
      "email": "a.rosenhagen@ffuenf.de"
    }
  ],
  "type": "magento-module",
  "require-dev": {
    "phpunit/phpunit": "4.*"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "http://packages.firegento.com"
    }
  ],
  "extra": {
    "magento-root-dir": "./magento/",
    "map": [
      [
        "app/code/community/Ffuenf/Canonical",
        "app/code/community/Ffuenf/Canonical"
      ],
      [
        "app/etc/modules/Ffuenf_Canonical.xml",
        "app/etc/modules/Ffuenf_Canonical.xml"
      ],
      [
        "app/design/adminhtml/default/default/layout/ffuenf/canonical.xml",
        "app/design/adminhtml/default/default/layout/ffuenf/canonical.xml"
      ],
      [
        "app/design/adminhtml/default/default/template/canonical",
        "app/design/adminhtml/default/default/template/canonical"
      ],
      [
        "app/design/frontend/default/default/layout/ffuenf/canonical.xml",
        "app/design/frontend/default/default/layout/ffuenf/canonical.xml"
      ],
      [
        "app/design/frontend/default/default/template/canonical",
        "app/design/frontend/default/default/template/canonical"
      ]
    ]
  }
}